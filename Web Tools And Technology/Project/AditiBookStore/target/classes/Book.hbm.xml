<hibernate-mapping>
  <class name="com.aditicompany.pojo.Book" table="book">
    <id name="bookId" column="bookId" type="int">
      <generator class="native"/>
    </id>
    <property name="movieTitle" column="title" type="string"/>
    <property name="leadActor" type="java.lang.String">
        <column name="actor"/>
    </property>
    <property name="leadActress" column="actress" type="string"/>
    <property name="genre" column="genre" type="string"/>
    <property name="year" column="year" type="int"/>
  </class>
</hibernate-mapping>